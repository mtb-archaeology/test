@techreport{potter_archeological_1992,
	title = {Archeological {Investigations} {Conducted} for the {General} {Services} {Administration} at the {Gateway} {Border} {Station} {Expansion} {Project}, {Phase} {II}; {Brownsville}, {Texas}},
	abstract = {The General Services Administration (GSA) in consultation with the National Park Services Interagency Acheological Services Division, Southeast Region (IASD), and the Texas State Historic Preservation Officer (SHPO) contracted with the National Park Services Southeast Archeological Center (SEAC), to conduct archeological investigations at Fort Brown, Brownsville, Texas, in compliance with Section 106 of the National Historic Preservation Act of 1966. The expanded commerce between the United States and Mexico has made the present border facilities inadequate to efficently handle the expanded intercountry trade traffic. Therfore, GSA is implementing a multi-phase construction program to upgrade the present facilities. The acquisition of a section of land owned by the City of Brownsville for an extensive expansion of the U.S. Custom Inspection Docks and Administration facilities at the International Gateway Bridge Border Crossing, necessitated archeological testing to determine whether extant and intact cultural and/or structural materials still existed under the present ground surface. Archeological investigations prior to the demolition of a 30,000 square feet convention pavilion with associated parking lots and green areas was considered advisable as no previous archeological work had been done within the area of proposed construction. Archeological testing concentrated in green areas located around the pavilion and a south extension of the subject property. Historic information and archeological data have been combined to provide a documentary and material record of this area of Fort Brown for the 143 years of human occupation.},
	author = {Potter, Douglas T. and Schwadron, Margo},
