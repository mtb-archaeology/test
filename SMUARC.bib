@techreport{banks_child_1982,
	title = {A child bundle burial from {Val} {Verde} {County}, {Texas}},
	abstract = {A prehistoric child bundle burial recovered from a cave in West Texas has presented an opportunity for detailed analysis of physiology and burial practices.  The infant was wrapped in several layers of mats and netting and secured with cordage and leather thongs.  Ten wooden sticks, some of which bear direct evidence of human alteration, also accompanied the bundle.  With little exception, the skeleton is complete, and anthropometric measurements are not at variance with other Trans-Pecos examples.  The mat construction provides a rough means for dating (1000 B.C. to A.D. 1000), and the bundle preparation provides information on burial practices in West Texas.  The burial is discussed in the context of the regional archaeology.},
	language = {English},
	author = {Banks, Kimball M. and Rutenberg, Gary Wayne},
	year = {1982},
	note = {Prepared for the Texas Archaeological Foundation.},
	keywords = {Texas, Val Verde County},
}
