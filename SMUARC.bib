@techreport{jurney_archaeological_1994,
	address = {Erie, Pennsylvania},
	title = {Archaeological {Investigations} at {41DL279}, {Site} of the {John} {F}. {Kennedy} {Exhibit}, {Dallas} {County} {Administrative} {Building}, {Dallas}, {Texas}},
	shorttitle = {Archaeological {Investigations} at {41DL279}},
	url = {https://core.tdar.org/document/295549/archeological-investigations-at-41dl279-site-of-the-john-f-kennedy-exhibit-dallas-county-administration-building-dallas-texas},
	abstract = {The archaeological investigations of the proposed excavation and construction area for the John. F. Kennedy Exhibit addition to the Dallas County Administration Building (otherwise known as the Texas School Book Depository), Dallas, Texas, were conducted from late December 1987 to February 1988. A background literature search and initial archaeological reconnaissance of the study area-which encompasses Dallas' Block 10/13 and has been designated as archaeological site 41DL279-were performed by the Archaeology Research Program, Southern Methodist University. The field investigations comprised three phases of work: (1) the machine-assisted removal of relatively recent overburden from the site's historical component; (2) archaeological testing of the exposed nineteenth-century landscape conducted by Freese and Nichols Inc., under Texas Antiquities Permit 671, Susan Andrews, Principal Investigator; and (3) intensive mitigation excavations conducted by the Archaeology Research Program, Southern Methodist University, under Texas Antiquities Permit 671-A, Randall W. Moir and David H. Jurney, Co-Principal Investigators for the project's field phase. This report was written, edited, and produced by the Archaeology Research Program, Mercyhurst Archaeological Institute, Mercyhurst College, under the direction of J. M. Adovasio, Ph.D., Principal Investigator for the project. These archaeological investigations at 41DL279, which are the first to be conducted within the Central Business District of Dallas, Texas, indicate that significant historic and prehistoric archaeological deposits are preserved under urban fills in portions of the city. The investigations reported herein identified and assessed material culture remains, building remnants, and support structures associated with the Dallas's formative period, beginning in the late 1840s or 1850s and extending through the Civil War and Reconstruction into the first decade of the railroad's presence in 1870s Dallas. Specific features in this landscape include: the foundation remains of original block subdivision fences and ditches made by John Neely Bryan sometime between 1844 and 1849; at least two dwellings and a well dating ca. 1853-1856 which are associated with the prominent early Dallas family of the French immigrant Maxime Guillot; the back yards and privies of African American slaves(?), tenants, and laundresses on lots owned by another prominent family, that of George and Mary Baird; and three fences separating the lots occupied by these individuals. The material culture deposits represent serial domestic occupations from ca. 1850 to ca. 1882. The report also describes and discusses several features associated with the machine-stripped deposits which overlay the ca. 1850-1882 component. These later features-attributed a stoneworks dating to the 1890s and rail yards dating ca. 1882-present-include two privies from this early industrial phase of Dallas's urban development. The temporal span represented by these cultural deposits saw Dallas change from a quiet village to a thriving city.},
	language = {English},
	institution = {Archaeology Research Program of Mercyhurst Archaeological Institute, Mercyhurst College},
	author = {Jurney, David H. and Andrews, Susan L.},
	collaborator = {Dirkmaat, Dennis C. and Dorward, Donald and Green, Melissa M. and Kubas, Cassandra and McElhaney, Jackie and Moir, Randall W. and Payton, Donald and Pedler, Catherine L. and Yates, Bonnie C.},
	year = {1994},
	note = {Texas Antiquities Permits 671 and 671-A.
ARC Library \#176},
	keywords = {Dallas County, Texas},
	annote = {Prepared for Southern Methodist University and the Dallas County Historical Foundation
},
	annote = {Work started on this project under the ARP, and SMU retains the artifacts.
},
}

