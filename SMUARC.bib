@book{bruseth_archaeological_1977,
	address = {Dallas, Texas},
	series = {Research {Report}},
	title = {Archaeological {Research} at {Lake} {Fork} {Reservoir}: {Site} {Inventory} and {Assessment}},
	abstract = {During portions of 1975 an archaeological survey was con-ducted on 27,690 acres of land which will be inundated by construction of Lake Fork Reservoir, northeastern Texas. One hundred thirty sites were located. Testing, consisting of surface collection, postholes, and/or test pits, was conducted at 65 of the sites located during survey. The purpose of this work was to assess the archaeological resources of the reservoir and to make recommendations for mitigation.

Occupation of the reservoir spans the Archaic (3500 B.C. to A.D. 500), Neo-American (A.D. 500 to 1650), and Historic (A.D. 1650 to the present) periods. Locational analysis demonstrated that sites with predominantly lithic assemblages are located mostly along and within Lake Fork floodplain, and sites with predominantly ceramic assemblages are located primarily along tributaries of Lake Fork Creek.Two models of prehistoric subsistence-settlement systems are offered to explain the distribution of lithic and ceramic sites. Thirteen sites are recommended for further work in order to mitigate the ad-verse impact upon the archaeological resources by reservoir inundation.},
	language = {English},
	number = {87},
	publisher = {Archaeology Research Program, Department of Anthropology, Southern Methodist University},
	author = {Bruseth, James E. and Bagot, Joe T. and Banks, Kimball M. and McKinley, Mary A.},
