@techreport{hyatt_survey_1972,
	address = {Dallas, Texas},
	type = {Research {Report}},
	title = {Survey of {Cultural} {Resources} of the {Proposed} {Big} {Pine} {Lake}, {Texas}},
	url = {https://core.tdar.org/document/29989/survey-of-cultural-resources-of-the-proposed-big-pine-lake-texas},
	abstract = {One hundred and sixteen cultural sites were located and recorded within the area of the proposed Big Pine Lake, Lamar and Red River Counties, northeast Texas. Sites occur in five geographic situations and are divided into two time periods, Archaic and Caddo.  Analysis of lithic samples from each site is correlated with site distribution in an attempt to determine the location of specific maintenance activities. The report concludes with a statement of the environmental impact of the lake construction on the cultural resources present in the area, and a program designed to salvage these resources prior to dam construction and inundation is proposed.},
	number = {15},
	institution = {River Basin Salvage Project, Department of Anthropology, Southern Methodist University},
	author = {Hyatt, Robert D. and Mosca, Herbert P.},
